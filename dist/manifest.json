{
"manifest_version": 2,

  "name": "Coder's Calendar",
  "short_name": "Coder's Calendar",
  "description": "Never miss a Coding contest again! Displays live and upcoming Programming Contests happening on a variety of popular platforms.",
  "version": "1.3.0",

  "permissions": [
    "http://contesttrackerapi.herokuapp.com/",
    "notifications",
    "storage"
  ],
  "icons": {
    "16": "img/icon32.png",
    "32": "img/icon32.png",
    "48": "img/icon32.png",
    "128": "img/icon32.png"
  },
  "browser_action": {
    "default_icon": "img/icon32.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["js/buy.js", "js/notifier_service.js", "js/event_listeners.js"]
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}
